{
  "name": "@nimbletank/react-components",
  "version": "0.1.0",
  "description": "Components used in web projects at Nimbletank",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format:js": "prettier-standard 'components/**/*.js' '.storybook/**/*.js'",
    "precommit": "lint-staged",
    "postinstall": "npm run build",
    "storybook:run": "BABEL_ENV=test start-storybook -p 9001 -c .storybook"
  },
  "lint-staged": {
    "linters": {
      "(components|.storybook)/**/*.js": [
        "prettier-standard",
        "test",
        "git add"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nimbletank/react-components"
  },
  "keywords": [
    "react",
    "react.js",
    "components"
  ],
  "author": "Zander Martineau",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.14",
    "@storybook/addon-info": "^3.2.14",
    "@storybook/addon-knobs": "^3.2.14",
    "@storybook/addon-options": "^3.2.14",
    "@storybook/react": "^3.2.18",
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.1",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^4.10.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^4.3.0",
    "postcss-loader": "^2.0.8",
    "prettier": "^1.8.1",
    "prettier-standard": "^7.0.3",
    "raw-loader": "^0.5.1",
    "react-dom": "^16.1.0",
    "react-test-renderer": "^16.0.0",
    "storybook-addon-jsx": "^5.0.0",
    "storybook-readme": "^3.1.1",
    "style-loader": "^0.19.0"
  }
}
